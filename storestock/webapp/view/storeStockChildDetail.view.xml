<mvc:View controllerName="com.agel.mmts.storestock.controller.storeStockChildDetail" height="100%"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:f="sap.f">
    <f:DynamicPage headerExpanded="true" busy="{objectViewModel>/busy}">
        <!-- DynamicPage Title -->
        <f:title>
            <f:DynamicPageTitle>
                <f:heading>
                    <Title id="pageTitle" text="{i18n>Store-Stock-Child}"/>
                </f:heading>
                 <f:actions>
                   <Button text="{i18n>Close}" press="onCloseChildPage" type="Emphasized"/>
                </f:actions>
            </f:DynamicPageTitle>
        </f:title>
        <!-- DynamicPage Header -->
        <f:header>
            <f:DynamicPageHeader pinnable="true">
                <f:content>
                    <core:Fragment fragmentName="com.agel.mmts.storestock.view.fragments.ChilddetailPage.FilterBar" type="XML"/>
                </f:content>
            </f:DynamicPageHeader>
        </f:header>
        <!-- DynamicPage Content -->
        <f:content>
           

            <core:Fragment fragmentName="com.agel.mmts.storestock.view.fragments.ChilddetailPage.StoreStockChild" type="XML"/>
        </f:content>
    </f:DynamicPage>
</mvc:View>

