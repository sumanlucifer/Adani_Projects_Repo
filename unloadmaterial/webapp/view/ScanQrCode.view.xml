<mvc:View xmlns:vbm="sap.ui.vbm"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:uxap="sap.uxap"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:unified="sap.ui.unified"
    xmlns:tt="sap.ui.table"
    xmlns:scan="sap.ndc"
    xmlns="sap.m" controllerName="com.agel.mmts.unloadmaterial.controller.ScanQrCode" height="100%">
    <uxap:ObjectPageLayout id="ObjectPageLayout" showTitleInHeaderContent="true" upperCaseAnchorBar="false" headerContentPinnable="true" toggleHeaderOnTitleClick="true" showFooter="true">
        <uxap:headerTitle>
            <uxap:ObjectPageDynamicHeaderTitle>
                <uxap:content>
                    <!-- <HBox alignItems="Center">
                        <Text text="Domestic Entry"/>
                    </HBox> -->
                    <HBox alignItems="Center">
                        <!-- <Input id="idInputQRCode" width="100%" showSuggestion="true" showValueHelp="false" suggestionItems="{/QRCodeSet}" placeholder="Enter QR Number" valueLiveUpdate="true" suggestionItemSelected="onQRCodeSuggestionSelected" liveChange="onQRCodeSuggestionSelected" class="sapUiSmallMarginEnd" enabled="{oViewModel>/inputQRCode}">
                            <suggestionItems>
                                <core:Item text="{QRNumber}" key="{QRNumber}" />
                            </suggestionItems>
                        </Input> -->
                         <Input id="idInputQRCode" width="100%" showSuggestion="false" showValueHelp="false" placeholder="Enter QR Number" valueLiveUpdate="true" liveChange="onQRCodeSuggestionSelected" class="sapUiSmallMarginEnd" enabled="{oViewModel>/inputQRCode}"/>
                        <Button id="idQRBtn" text="{i18n>submit}" type="Emphasized" press="onPressSubmitQRCode" class="sapUiSmallMarginBegin" enabled="{oViewModel>/submitQRCode}"/>
                    </HBox>
                    <HBox alignItems="Center">
                        <Text text="{i18n>or}" class="sapUiTinyMargin"/>
                    </HBox>
                    <HBox alignItems="Center">
                        <scan:BarcodeScannerButton id="idQRScanBtn" scanSuccess="onScanSuccess" scanFail="onScanError"/>
                        <!-- <Button id="idQRSubmit" text="{i18n>scanQrCode}" type="Emphasized" press="onPressScanQRCode" enabled="{oViewModel>/scanQRCode}"/> -->
                    </HBox>
                    <HBox alignItems="Center">
                        <Text text="{i18n>OR}" class="sapUiTinyMargin"/>
                    </HBox>

                    <HBox alignItems="Center">
                        <!-- <Input id="idInvoiceNum" width="100%" valueLiveUpdate="true" suggestionItems="{/PackingListSet}" showSuggestion="true" placeholder="Enter Invoice Number" showValueHelp="false" class="sapUiSmallMarginEnd" suggestionItemSelected="onInvoiceSuggestionSelected" liveChange="onInvoiceSuggestionSelected" enabled="{oViewModel>/inputInvoiceCode}">
                            <suggestionItems>
                                <core:Item text="{InvoiceNumber}" key="{InvoiceNumber}" />
                            </suggestionItems>
                        </Input> -->
                        <Input id="idInvoiceNum" width="100%" valueLiveUpdate="true" showSuggestion="false" placeholder="{i18n>EnterInvoiceNumber}" showValueHelp="false" class="sapUiSmallMarginEnd" liveChange="onInvoiceSuggestionSelected" enabled="{oViewModel>/inputInvoiceCode}"/>
                        <Button id="idInvBtn" text="{i18n>submit}" type="Emphasized" press="onPressSubmitInvoiceNumber" class="sapUiSmallMarginBegin" enabled="{oViewModel>/submitInvoiceCode}"/>
                    </HBox>
                </uxap:content>
            </uxap:ObjectPageDynamicHeaderTitle>
        </uxap:headerTitle>
        <uxap:headerContent>
        </uxap:headerContent>
        <uxap:sections>
        </uxap:sections>
    </uxap:ObjectPageLayout>
</mvc:View>