<core:FragmentDefinition xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.ui.layout.form"
    xmlns="sap.m"
    xmlns:smartTable="sap.ui.comp.smarttable"
    xmlns:tt="sap.ui.table">
<Table id="idOuterPackaging" items="{path: 'OuterPackagings'}" autoPopinMode="true" growingThreshold="5" growing="true" fixedLayout="false">
        <columns>
            <Column>
                <Text text="{i18n>OuterPackaging}"/>
            </Column>
            <!-- <Column>
                <Text text="Line Item ID"/>
            </Column> -->
            <!-- <Column>
                <Text text="Dispatch Qty."/>
            </Column> -->
            <Column>
                <Text text="{i18n>PackagingDimensions}"/>
            </Column>
            <!-- <Column>
                <Text text="UOM"/>
            </Column> -->
            <Column>
               <Text text="{i18n>Action}"/>
            </Column>
        </columns>
        <items>
            <ColumnListItem>
                <cells>
                    <Text text="{PackagingType}"/>
                    <!-- <ObjectIdentifier title="{ID}"/> -->
                    <!-- <Text text="{DispatchQty}"/> -->
                    <Text text="{PackagingDimensions}"/>
                    <!-- <Text text="{UOM}"/> -->
                    <Link text="View QR Code" binding="{QrCode}" visible="{= ${QRNumber} !== '' ? true :false}" press="onViewQRCodePress"/>
                </cells>
            </ColumnListItem>
        </items>
    </Table>
</core:FragmentDefinition>