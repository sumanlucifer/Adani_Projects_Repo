<mvc:View xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.ui.layout.form"
    xmlns="sap.m"
    xmlns:smartTable="sap.ui.comp.smarttable"
    xmlns:scan="sap.ndc"
    xmlns:tt="sap.ui.table" controllerName="com.agel.mmts.unloadmaterial.view.blocks.UnloadMaterial.innerPackaging.InnerPackagingController">
    
  <Table id="idInnerPackaging" items="{JSONModelData>/InnerPackings}" autoPopinMode="true" growingThreshold="5" growing="true" fixedLayout="false" alternateRowColors="true">
        <columns>
            <Column>
                <Text text="{i18n>InnerPackaging}"/>
            </Column>
            <Column>
                <Text text="{i18n>lineItem}"/>
            </Column>
            <Column>
                <Text text="{i18n>PackagingQty}"/>
            </Column>
            <Column>
                <Text text="{i18n>ReceivedQty}"/>
            </Column>
            <Column>
                <Text text="{i18n>UOM}"/>
            </Column>
            <Column>
              <Text text="Map Vendor QR Code"/>
            </Column>
        </columns>
        <items>
            <ColumnListItem>
                <cells>
                    <Text text="{JSONModelData>InnerPackingLabel}"/>
                    <Text text="{JSONModelData>LineItemLabel}"/>
                    <Text text="{JSONModelData>PackagingQty}"/>
                    <Text text="{JSONModelData>ReceivedQty}"/>
                    <Text text="{JSONModelData>UOM}"/>
                    <!-- <core:Icon
                        src="sap-icon://sys-help-2" class="size1" binding="{QrCode}" visible="{= ${QRNumber}}" color="#031E48" press="onViewQRCodePress">
                        <core:layoutData>
                            <FlexItemData growFactor="1" />
                        </core:layoutData>
                    </core:Icon> -->
                    <!-- <core:Icon src="sap-icon://accept" class="size1" color="#087137" >
                        <core:layoutData>
                            <FlexItemData growFactor="1" />
                        </core:layoutData>
                    </core:Icon> -->
                    <!-- <Link visible="{= ${JSONModelData>ReceivedQty} ? true : false }" text="{= ${JSONModelData>ExternalQRCodeInfo} === '' ? 'Map Vendor QR Code' : 'Vendor Information Mapped' }" press="onMapVendorQRPress"/> -->
                    <scan:BarcodeScannerButton id="idQRSubmit" scanSuccess="onMapVendorQRSuccess" scanFail="onMapVendorQRFail" visible="{= ${JSONModelData>ReceivedQty} ? true : false }"/>   
                </cells>
            </ColumnListItem>
        </items>
    </Table> 
</mvc:View>


