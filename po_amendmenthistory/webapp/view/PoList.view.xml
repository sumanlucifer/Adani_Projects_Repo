<mvc:View
    controllerName="com.agel.mmts.poamendmenthistory.controller.PoList"
    height="100%"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns="sap.m"
	xmlns:f="sap.f">
	<f:DynamicPage headerExpanded="true" busy="{objectViewModel>/busy}">
		<!-- DynamicPage Title -->
		<f:title>
			<f:DynamicPageTitle>
				<f:heading>
					<Title id="pageTitle" text="{i18n>txtPOAmendmentHistory}"/>
				</f:heading>
			</f:DynamicPageTitle>
		</f:title>
		<!-- DynamicPage Header -->
		<f:header>
			<f:DynamicPageHeader pinnable="true">
				<f:content>
					<core:Fragment fragmentName="com.agel.mmts.poamendmenthistory.view.fragments.filterbar" type="XML"/>
				</f:content>
			</f:DynamicPageHeader>
		</f:header>
		<!-- DynamicPage Content -->
		<f:content>
            <core:Fragment fragmentName="com.agel.mmts.poamendmenthistory.view.fragments.POTable" type="XML"/>
		</f:content>
	</f:DynamicPage>
</mvc:View>
