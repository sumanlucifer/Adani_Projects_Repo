<mvc:View
    controllerName="com.agel.mmts.hodroleapproval.controller.LandingPage" height="100%"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
xmlns:f="sap.f">
    <f:DynamicPage headerExpanded="true" busy="{objectViewModel>/busy}">
        <!-- DynamicPage Title -->
        <f:title>
            <f:DynamicPageTitle>
                <f:heading>
                    <Title text="{i18n>appTitleNew}"/>
                </f:heading>
            </f:DynamicPageTitle>
        </f:title>
        <!-- DynamicPage Header -->
        <f:header>
            <f:DynamicPageHeader pinnable="true">
                <f:content>
                    <core:Fragment fragmentName="com.agel.mmts.hodroleapproval.view.fragments.filterbar" type="XML"/>
                </f:content>
            </f:DynamicPageHeader>
        </f:header>
        <!-- DynamicPage Content -->
        <f:content>
            <core:Fragment fragmentName="com.agel.mmts.hodroleapproval.view.fragments.UserTable" type="XML"/>
        </f:content>
    </f:DynamicPage>
</mvc:View>
