{
    "_version": "1.32.0",
    "sap.app": {
        "id": "com.agel.mmts.pmcg",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "ach": "ach",
        "dataSources": {
            "mainService": {
                "uri": "/AGEL_MMTS_API/api/v2/odata.svc/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/metadata.xml"
                }
            }
        },
        "crossNavigation": {
            "inbounds": {
                "com-agel-mmts-pmcg-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "mdccRequests",
                    "action": "manage",
                    "title": "MDCC Requests",
                    "subTitle": "",
                    "icon": ""
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://task",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "com.agel.mmts.pmcg.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.66.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.agel.mmts.pmcg.i18n.i18n"
                }
            },
            "": {	
                "type": "sap.ui.model.odata.v2.ODataModel",	
                "settings": {	
                    "defaultOperationMode": "Server",	
                    "defaultBindingMode": "TwoWay",	
                    "defaultCountMode": "Request",	
                    "refreshAfterChange": true,	
                    "useBatch": false,
                    "defaultUpdateMethod":"PUT"
	
                },
                "dataSource": "mainService",
                "preload": true
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "com.agel.mmts.pmcg.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false,
                "bypassed": {
                    "target": [
                        "notFound"
                    ]
                }
            },
            "routes": [
                {
                    "name": "RouteLandingPage",
                    "pattern": "",
                    "target": [
                        "TargetLandingPage"
                    ]
                },
                {	
                    "name": "TCEngDetailPage",	
                    "pattern": "TCEngDetail/{TCEngId}",	
                    "target": [	
                        "TCEngDetailsPage"	
                    ]	
                }
            ],
            "targets": {
                "TargetLandingPage": {
                    "viewType": "XML",
                    "transition": "slide",
                    "viewName": "LandingPage",
                    "clearControlAggregation": false,
                    "viewId": "MDCCRequestList"
                },
                 "TCEngDetailsPage": {	
                    "viewType": "XML",	
                    "transition": "slide",	
                    "viewName": "TCEngDetail",	
                    "clearControlAggregation": false,
                    "viewLevel": 2	
                },
                "notFound": {
                    "viewName": "NotFound"
                }
            }
        }
    }
}