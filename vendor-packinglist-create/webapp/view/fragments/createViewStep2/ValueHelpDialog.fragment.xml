<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <TableSelectDialog 
        noDataText="{i18n>Noinnerpackagingavalable}" 
        title="{i18n>SelectInnerPackagings}" search=".handleSearch" confirm="onSaveInnerPackagingListPress" 
        confirmButtonText="{i18n>Save}" cancel=".handleValueHelpClose" showClearButton="true" multiSelect="true" 
        items="{
			path : 'valueHelpModel>/'
		}">
        <ColumnListItem selected="{valueHelpModel>selected}">
            <cells>
                <ObjectIdentifier title="{valueHelpModel>PackagingType}" />
                <Text text="{valueHelpModel>Name}"></Text>
                <Text text="{valueHelpModel>PackagingQty}"></Text>
                <Text text="{valueHelpModel>UOM}"></Text>
            </cells>
        </ColumnListItem>
        <columns>
            <Column>
                <Text text="{i18n>PackagingType}" />
            </Column>
            <Column>
                <Text text="{i18n>MaterialName}" />
            </Column>
            <Column hAlign="Center">
                <Text text="{i18n>PackagingQuantity}" />
            </Column>
            <Column>
                <Text text="{i18n>UOM}" />
            </Column>
        </columns>        
    </TableSelectDialog>
</core:FragmentDefinition>
