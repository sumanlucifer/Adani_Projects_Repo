<mvc:View xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.ui.layout.form"
    xmlns="sap.m"
    xmlns:smartTable="sap.ui.comp.smarttable"
    xmlns:tt="sap.ui.table" controllerName="com.agel.mmts.vendorpackinglistcreate.view.blocks.packingListDetails.innerPackaging.InnerPackagingController">
     <Table id="idInnerPackaging" items="{path: 'InnerPackagings'}" autoPopinMode="true" 
        growingThreshold="5" growing="true" fixedLayout="false">
        <columns>
            <Column>
                <Text text="{i18n>PackagingType}"/>
            </Column>
            <Column>
                <Text text="{i18n>MaterialName}"/>
            </Column>
            <!-- <Column>
                <Text text="Dispatch Qty."/>
            </Column> -->
            <Column>
                <Text text="{i18n>PackagingQuantity}"/>
            </Column>
            <Column>
                <Text text="{i18n>UOM}"/>
            </Column>
            <Column>
              <Text text="{i18n>Action}"/>
            </Column>
        </columns>
        <items>
            <ColumnListItem>
                <cells>
                    <ObjectIdentifier title="{PackagingType}"/>
                    <ObjectIdentifier title="{Name}"/>
                    <!-- <Text text="{DispatchQty}"/> -->
                    <Text text="{PackagingQty}"/>
                    <Text text="{UOM}"/>
                    <Link text="{i18n>ViewQRCode}" binding="{QrCode}" visible="{= ${QRNumber}}" press="onViewQRCodePress"/>
                </cells>
            </ColumnListItem>
        </items>
    </Table>    
</mvc:View>


