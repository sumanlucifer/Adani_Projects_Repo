<mvc:View controllerName="com.agel.mmts.tcengineer.controller.BOQRequestList" height="100%"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:f="sap.f">
    <f:DynamicPage headerExpanded="true" busy="{objectViewModel>/busy}">
        <!-- DynamicPage Title -->
        <f:title>
            <f:DynamicPageTitle>
                <f:heading>
                    <Title text="BOQ Approval Requests"/>
                </f:heading>
            </f:DynamicPageTitle>
        </f:title>
        <!-- DynamicPage Header -->
        <f:header>
            <f:DynamicPageHeader pinnable="true">
                <f:content>
                    <core:Fragment fragmentName="com.agel.mmts.tcengineer.view.fragments.BOQRequestList.FilterBar" type="XML"/>
                </f:content>
            </f:DynamicPageHeader>
        </f:header>
        <!-- DynamicPage Content -->
        <f:content>
            <core:Fragment fragmentName="com.agel.mmts.tcengineer.view.fragments.BOQRequestList.BOQRequestTable" type="XML"/>
        </f:content>
    </f:DynamicPage>
</mvc:View>
