{
    "_version": "1.32.0",
    "sap.app": {
        "id": "com.agel.mmts.vendormanageboq",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "ach": "ach",
        "dataSources": {
            "mainService": {
                "uri": "/api/v2/odata.svc/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/metadata.xml"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://task",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "com.agel.mmts.vendormanageboq.view.App",
            "type": "XML",
            "async": true,
            "id": "fcl"
        },
        "dependencies": {
            "minUI5Version": "1.66.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.f": {},
                "sap.ui.layout": {}
            }
        },
        "config": {
            "fullWidth": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.agel.mmts.vendormanageboq.i18n.i18n"
                }
            },
            "": {
                "type": "sap.ui.model.odata.v2.ODataModel",
                "settings": {
                    "defaultOperationMode": "Server",
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": true,
                    "useBatch": false
                },
                "dataSource": "mainService",
                "preload": true
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.f.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "com.agel.mmts.vendormanageboq.view",
                "controlAggregation": "pages",
                "controlId": "fcl",
                "clearControlAggregation": false,
                "transition": "slide",
                "bypassed": {
                    "target": []
                }
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "LandingPage",
                    "target": [
                        "TargetLandingPage"
                    ]
                },
                {
                    "pattern": "detail/{parentMaterial}/{layout}",
                    "name": "detail",
                    "titleTarget": "",
                    "greedy": false,
                    "target": [
                        "TargetLandingPage",
                        "detail"
                    ]
                },
                {
                    "pattern": "detail/{parentMaterial}/detailDetail/{pcList}/{layout}",
                    "name": "detailDetail",
                    "target": [
                        "TargetLandingPage",
                        "detail",
                        "detailDetail"
                    ]
                }
            ],
            "targets": {
                "TargetLandingPage": {
                    "viewName": "LandingPage",
                    "viewLevel": 1,
                    "viewType": "XML",
                    "transition": "slide",
                    "controlAggregation": "beginColumnPages"
                },
                "detail": {
                    "viewName": "Detail",
                    "viewLevel": 2,
                    "viewType": "XML",
                    "transition": "slide",
                    "controlAggregation": "midColumnPages"
                },
                "detailDetail": {
                    "viewName": "DetailDetail",
                    "viewLevel": 3,
                    "viewType": "XML",
                    "transition": "slide",
                    "controlAggregation": "endColumnPages"
                }
            }
        }
    }
}