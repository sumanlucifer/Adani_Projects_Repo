<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.ui.layout.form">
    
     <Dialog contentWidth="35%">
        <content>
            <MultiComboBox id="roleEdit" placeholder="{i18n>Select}" selectionFinish="onRoleSelectionFinish"
                items="{
                    path: 'objectViewModel>/UnassignRoleList'
                }">
                <core:Item key="{objectViewModel>ID}" text="{objectViewModel>RoleName}"/> 
            </MultiComboBox>
        </content>
        <beginButton>
            <Button id="idSaveNewRoleBTN" text="{i18n>Save}" press="onSave" type="Emphasized"/>
        </beginButton>
        <endButton>
            <Button text="{i18n>Close}" press="onClose" type="Emphasized" />
        </endButton>
    </Dialog>

</core:FragmentDefinition>