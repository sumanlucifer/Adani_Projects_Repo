<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.ui.layout.form">
    
     <Dialog contentWidth="35%">
        <content>
            <MultiComboBox id="roleEdit" placeholder="Select"
                items="{
                    path: '/MasterRoleSet'
                }">
                <core:Item key="{ID}" text="{RoleName}"/> 
            </MultiComboBox>
        </content>
        <beginButton>
            <Button text="Save" press="onSave" type="Emphasized" />
        </beginButton>
        <endButton>
            <Button text="Close" press="onClose" type="Emphasized" />
        </endButton>
    </Dialog>

</core:FragmentDefinition>