{
    "_version": "1.32.0",
    "sap.app": {
        "id": "com.agel.mmts.userroleassignment",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "ach": "ach",
        "crossNavigation": {
            "inbounds": {
                "com-agel-mmts-userroleassignment-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "userRoleAssignment",
                    "action": "manage",
                    "title": "User Role Assignment",
                    "subTitle": "",
                    "icon": ""
                }
            }
        },
        "dataSources": {
            "mainService": {
                "uri": "/AGEL_MMTS_API/api/v2/odata.svc/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/metadata.xml"
                }
            }
        } 
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://task",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "com.agel.mmts.userroleassignment.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.66.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.agel.mmts.userroleassignment.i18n.i18n"
                }
            },
            "": {
                "type": "sap.ui.model.odata.v2.ODataModel",	
                "settings": {	
                    "defaultOperationMode": "Server",	
                    "defaultBindingMode": "TwoWay",	
                    "defaultCountMode": "Request",	
                    "refreshAfterChange": true,	
                    "useBatch": false	
                },
                "dataSource": "mainService",
                "preload": true
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "com.agel.mmts.userroleassignment.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteUserList",
                    "pattern": "",
                    "target": [
                        "TargetUserList"
                    ]
                },
                {	
                    "name": "RouteUserRoleDetailPage",	
                    "pattern": "UserDetail/{BOQRequestId}",	
                    "target": [	
                        "TargetUserDetailsPage"	
                    ]	
                }
            ],
            "targets": {
                "TargetUserList": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "UserList",
                    "viewName": "UserList"
                },
                 "TargetUserDetailsPage": {	
                    "viewType": "XML",	
                    "transition": "slide",	
                    "clearControlAggregation": false,	
                    "viewName": "UserDetails",	
                    "viewLevel": 2	
                }	
            }
        }
    }
}