<mvc:View controllerName="com.agel.mmts.raiseconsumptionporequest.controller.RaiseConsumptionPage"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core" displayBlock="true"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:layout="sap.ui.layout" height="100%">

    <f:DynamicPage headerExpanded="true" busy="{objectViewModel>/busy}">
        <!-- DynamicPage Title -->
        <f:title>
            <f:DynamicPageTitle>
                <f:heading>
                    <Title id="pageTitle" text="{i18n>ConsumptionPosting}"/>
                </f:heading>
                <f:actions>
                    <!-- <Button text="Issue New Material" type="Emphasized" press="onpressIssueMaterial"/> -->
                </f:actions>
            </f:DynamicPageTitle>
        </f:title>
        <f:header>
        </f:header>
        <f:content>

            <IconTabBar id="idIconTabBar" select="onIconTabSelect" headerMode="Inline" expanded="true" class="sapUiResponsiveContentPadding">
                <items>

                    <IconTabFilter id="idIssueReservedTitle" text="{i18n>ConsumptionReservationTable}" key="IssueReservedKey" count="{oIconTabCountModel>/issuereservedCount}">
                        <core:Fragment fragmentName="com.agel.mmts.raiseconsumptionporequest.view.fragments.IssueReservedTable" type="XML"/>
                    </IconTabFilter>

                    <IconTabFilter id="idIssueTitle" text="{i18n>ConsumptionPostingTable}" key="IssueKey" count="{oIconTabCountModel>/issueCount}">
                        <core:Fragment fragmentName="com.agel.mmts.raiseconsumptionporequest.view.fragments.ListTable" type="XML"/>
                    </IconTabFilter>


                </items>
            </IconTabBar>

        </f:content>
    </f:DynamicPage>
</mvc:View>

