<mvc:View xmlns:f2="sap.f"
    xmlns:vbm="sap.ui.vbm"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:uxap="sap.uxap"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:unified="sap.ui.unified"
    xmlns:ui="sap.ui.table"
    xmlns:smartTable="sap.ui.comp.smarttable"
    xmlns="sap.m" controllerName="com.agel.mmts.raiseconsumptionporequest.controller.RaiseConsumptionCancelPage" height="100%">
    <uxap:ObjectPageLayout id="ObjectPageLayout" showTitleInHeaderContent="true" upperCaseAnchorBar="false" headerContentPinnable="true" toggleHeaderOnTitleClick="true" busy="{objectViewModel>/busy}">
        <uxap:headerTitle>
            <uxap:ObjectPageDynamicHeaderTitle>
                <uxap:breadcrumbs>
                    <Breadcrumbs id="breadcrumbsId" currentLocationText="{i18n>PostedConsumptionDetail}">
                        <Link text="{i18n>PostedConsumptions}" press=".handleToAllPOBreadcrumPress"/>
                    </Breadcrumbs>
                </uxap:breadcrumbs>
                <uxap:expandedHeading>
                    <FlexBox direction="Column" renderType="Bare">
                        <Title text="{i18n>ConsumptionPostingDetail}" wrapping="true" titleStyle="H4" class="sapUiTinyMarginTopBottom"/>
                        <Title text="{MaterialDocumentNumber}" wrapping="true" titleStyle="H3" />
                    </FlexBox>
                </uxap:expandedHeading>
                <uxap:snappedHeading>
                    <FlexBox direction="Column" renderType="Bare">
                        <Title text="{i18n>ConsumptionPostingDetail}" wrapping="true" titleStyle="H4" class="sapUiTinyMarginTopBottom"/>
                        <Title text="{MaterialDocumentNumber}" wrapping="true" titleStyle="H3"/>
                    </FlexBox>
                </uxap:snappedHeading>
                <uxap:expandedContent>
                </uxap:expandedContent>
                <uxap:snappedTitleOnMobile>
                    <Title text="{i18n>MaterialDocumentNumber}- {MaterialDocumentNumber}" wrapping="true" titleStyle="Auto"/>
                </uxap:snappedTitleOnMobile>
                <uxap:actions>
                        <!-- <Button text="Done" press="onPressDone" type="Accept" visible="{objectViewModel>/doneButton}"/> -->
                        <Button visible="{= ${Status} === 'CONSUMPTION SUCCESSFUL' ? true : ${Status} === 'CANCELLATION FAILED' ? true : ${Status} === 'PARTIALLY CANCELLED' ? true : false }" text="Cancel consumption posting" press="onPressCancelConsumptionPosting" type="Emphasized"/>
                </uxap:actions>
            </uxap:ObjectPageDynamicHeaderTitle>
        </uxap:headerTitle>
        <uxap:headerContent>
            <FlexBox wrap="Wrap" height="100%" renderType="Bare" direction="Column">
                <VBox binding="{ConsumptionPostingReserve}">
                </VBox>
                <VBox renderType="Bare">
                    <FlexBox direction="Row" renderType="Bare" class="sapUiTinyMarginBottom">
                        <VBox renderType="Bare" class="sapUiMediumMarginEnd">
                            <Label text="{i18n>GatePassNumber}" />
                            <!-- missing -->
                            <Text text="{GatePassNumber}"/>
                        </VBox>
                        <VBox renderType="Bare" class="sapUiMediumMarginEnd">
                            <Label text="{i18n>issuedDate}" />
                            <!-- missing -->
                            <Text text="{path:'CreatedAt',
                                    type: 'sap.ui.model.type.DateTime',
                                    formatOptions: {
                                    pattern: 'MMMM dd, yyyy HH:mm:ss'
                                    }
                            }"/>
                        </VBox>
                        <VBox renderType="Bare" class="sapUiMediumMarginEnd">
                            <Label text="{i18n>PostingDate}" />
                            <!-- missing -->
                            <Text text="{path:'PostingDate',
                                        type: 'sap.ui.model.type.DateTime',
                                        formatOptions: {
                                        pattern: 'MMMM dd, yyyy HH:mm:ss'
                                        }
                            }"/>
                        </VBox>
                        <VBox renderType="Bare" class="sapUiMediumMarginEnd">
                            <Label text="{i18n>ReservationNumber}" />
                            <!-- missing -->
                            <Text text="{ReservationNumber}"/>
                        </VBox>
                         <VBox renderType="Bare" class="sapUiMediumMarginEnd">
                            <Label text="{i18n>status}" />
                            <!-- missing -->
                            <ObjectStatus text="{Status}" state="{= ${Status} === 'CONSUMPTION SUCCESSFUL' ? 'Success' : ${Status} === 'PARTIALLY CANCELLED' ? 'Information' : 'Error'}" visible="{= ${Status}.length>0 ? true : false }"/>
                        </VBox>
                    </FlexBox>
                </VBox>
                <!-- <VBox renderType="Bare">
                    <Label text="{i18n>instruction}" design="Bold"/>
                    <ObjectAttribute text="Please raise the inspection call through SIMS portal." />
                </VBox> -->
            </FlexBox>
        </uxap:headerContent>
        <uxap:sections>
            <!-- Line Items Section -->
            <uxap:ObjectPageSection titleUppercase="false">
                <uxap:subSections>
                    <uxap:ObjectPageSubSection >
            <VBox>
                <ui:TreeTable  id="TreeTable" rows="{path:'ConsumptionCancelTreeDataModel>/', parameters: {numberOfExpandedLevels: 2,arrayNames:['results']}}" selectionMode="None" enableSelectAll="false" ariaLabelledBy="title" rowsUpdated="onRowsUpdated">
                    <ui:columns>

                        <ui:Column>
                            <Label text="{i18n>Name}" />
                            <ui:template>
                                <Text text="{ConsumptionCancelTreeDataModel>Name}" wrapping="false" />
                            </ui:template>
                        </ui:Column>
                        <ui:Column>
                            <Label text="{i18n>Description}" />
                            <ui:template>
                                <Text text="{ConsumptionCancelTreeDataModel>Description}" wrapping="false" />
                            </ui:template>
                        </ui:Column>
                        <ui:Column>
                            <Label text="{i18n>MaterialCode}" />
                            <ui:template>
                                <Text text="{ConsumptionCancelTreeDataModel>MaterialCode}" wrapping="false" />
                            </ui:template>
                        </ui:Column>
                        <ui:Column>
                            <Label text="{i18n>BaseUnit}" />
                            <ui:template>
                                <Text text="{ConsumptionCancelTreeDataModel>BaseUnit}" wrapping="false" />
                            </ui:template>
                        </ui:Column>
                        <ui:Column>
                            <Label text="{i18n>UOM}" />
                            <ui:template>
                                <Text text="{ConsumptionCancelTreeDataModel>UOM}" wrapping="false" />
                            </ui:template>
                        </ui:Column>
                        <ui:Column>
                            <Label text="{i18n>Description}" />
                            <ui:template>
                                <Text text="{ConsumptionCancelTreeDataModel>Description}" wrapping="false" />
                            </ui:template>
                        </ui:Column>
                        <ui:Column>
                            <Label text="{i18n>StorageLocation}" />
                            <ui:template>
                                <Text text="{ConsumptionCancelTreeDataModel>StorageLocation}" wrapping="false" />
                            </ui:template>
                        </ui:Column>
                        <ui:Column>
                            <Label text="{i18n>BaseQty}" />
                            <ui:template>
                                <Text text="{ConsumptionCancelTreeDataModel>BaseQty}" wrapping="false" />
                            </ui:template>
                        </ui:Column>
                        <ui:Column>
                            <Label text="{i18n>IssuedQuantity}" />
                            <ui:template>
                                <Text text="{ConsumptionCancelTreeDataModel>IssuedQty}" wrapping="false" />
                            </ui:template>
                        </ui:Column>
                          <ui:Column>
                            <Label text="{i18n>WeightPerPiece}" />
                            <ui:template>
                                <Text text="{ConsumptionCancelTreeDataModel>WeightPerPiece}" wrapping="false" />
                            </ui:template>
                        </ui:Column>
                        <ui:Column>
                            <Label text="{i18n>BalancedQuantity}" />
                            <ui:template>
                                <Text text="{ConsumptionCancelTreeDataModel>BalanceQty}" wrapping="false" />
                            </ui:template>
                        </ui:Column>
                        <ui:Column>
                            <Label text="{i18n>ReservedQuantity}" />
                            <ui:template>
                                <Text   text="{ConsumptionCancelTreeDataModel>Quantity}" />
                            </ui:template>
                        </ui:Column>
                        <ui:Column>
                            <Label text="{i18n>Status}" />
                            <ui:template>
                           <ObjectStatus text="{ConsumptionCancelTreeDataModel>Status}" state="{= ${ConsumptionCancelTreeDataModel>Status} === 'CONSUMPTION SUCCESSFUL' ? 'Success' : ${Status} === 'CONSUMPTION CANCELLED' ? 'Information' : 'Error'}" visible="{= ${Status}.length>0 ? true : false }"/>
                            </ui:template>
                        </ui:Column>
                    </ui:columns>
                </ui:TreeTable>
            </VBox>
            </uxap:ObjectPageSubSection>
            </uxap:subSections>
            </uxap:ObjectPageSection>
        </uxap:sections>
    </uxap:ObjectPageLayout>
</mvc:View> 