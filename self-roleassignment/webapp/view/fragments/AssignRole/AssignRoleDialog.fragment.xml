<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.ui.layout.form">
    
     <Dialog contentWidth="35%">
        <content>
            <MultiComboBox id="roleEdit" placeholder= "{i18n>Select}" selectedKeys="{MasterRoleSet>/ID}"
            selectionChange="onNewUserRoleSelected"
                items="{
                    path: '/MasterRoleSet',
                    filters: [
                        { path: 'IsOrganizationRole', operator: 'EQ', value1: true}
                    ]
                }">
                <core:Item key="{ID}" text="{RoleName}"/> 
            </MultiComboBox>
            <!-- <MultiComboBox id="roleEdit" placeholder= "Select" selectedKeys="{MasterRoleSet>/ID}"
                items="{UserRolesModel>/}">
                <core:Item key="{UserRolesModel>ID}" text="{UserRolesModel>RoleName}"  enabled="{= ${UserRolesModel>Enabled} === true}"/> 
            </MultiComboBox> -->
        </content>
        <beginButton>
            <Button id="idSaveBTN" enabled="false" text="{i18n>Save}" press="onSave" type="Emphasized" />
        </beginButton>
        <endButton>
            <Button text="{i18n>Close}" press="onClose" type="Emphasized" />
        </endButton>
    </Dialog>

</core:FragmentDefinition>