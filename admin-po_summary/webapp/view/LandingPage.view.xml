<mvc:View
    controllerName="com.agel.mmts.adminposummary.controller.LandingPage"
    height="100%"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns="sap.m"
	xmlns:f="sap.f">
	<f:DynamicPage headerExpanded="true" busy="{objectViewModel>/busy}">
		<!-- DynamicPage Title -->
		<f:title>
			<f:DynamicPageTitle>
				<f:heading>
					<Title id="pageTitle" text="{i18n>POs}"/>
				</f:heading>
			</f:DynamicPageTitle>
		</f:title>
		<!-- DynamicPage Header -->
		<f:header>
			<f:DynamicPageHeader pinnable="true">
				<f:content>
					<core:Fragment fragmentName="com.agel.mmts.adminposummary.view.fragments.FilterBar" type="XML"/>
				</f:content>
			</f:DynamicPageHeader>
		</f:header>
		<!-- DynamicPage Content -->
		<f:content>
            <core:Fragment fragmentName="com.agel.mmts.adminposummary.view.fragments.OpenPOTable" type="XML"/>
			<!-- <IconTabBar id="idIconTabBar" select="onIconTabSelect" headerMode="Inline" expanded="true" class="sapUiResponsiveContentPadding">
                <items>
                    <IconTabFilter id="idOpenPOTabTitle" text="{i18n>OpenPO}" key="OPEN" count="{oIconTabCountModel>/openCount}">
                       
                    </IconTabFilter>
                </items>
            </IconTabBar> -->
		</f:content>
	</f:DynamicPage>
</mvc:View>
