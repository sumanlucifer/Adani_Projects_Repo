{
    "_version": "1.32.0",
    "sap.app": {
        "id": "com.agel.mmts.securityscanqr",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "ach": "ach",
        "sourceTemplate": {
            "id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
            "version": "1.40.12"
        },
        "crossNavigation": {
            "inbounds": {
                "intent1": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "securityScanQR",
                    "action": "manage",
                    "title": "{{appTileTitle}}",
                    "subTitle": "{{appTileSubTitle}}",
                    "icon": "sap-icon://supplier"
                    
                }
            }
        },
        "dataSources": {
            "default": {
                "uri": "/AGEL_MMTS_API/api/v2/odata.svc/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/metadata.xml"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://task",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "com.agel.mmts.securityscanqr.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        },
        "dependencies": {
            "minUI5Version": "1.66.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.agel.mmts.securityscanqr.i18n.i18n"
                }
            },
            "": {
                "type": "sap.ui.model.odata.v2.ODataModel",
                "settings": {
                    "defaultOperationMode": "Server",	
                    "defaultBindingMode": "TwoWay",	
                    "defaultCountMode": "Inline",	
                    "refreshAfterChange": true,	
                    "useBatch": false	
                },
                "dataSource": "default",
                "preload": true
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "com.agel.mmts.securityscanqr.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "ScanQrCodePage",
                    "pattern": "",
                    "target": [
                        "ScanQRCodePage"
                    ]
                },
                {
                    "name": "QRCodeDetailsPage",
                    "pattern": "QRCodeDetails/{QRNo}/{Type}",
                    "target": [
                        "QRCodeDetailsPage"
                    ]
                }
            ],
            "targets": {
                "ScanQRCodePage": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "ScanQrCode",
                    "viewName": "ScanQrCode",
                    "viewLevel": 1
                },
               
                "QRCodeDetailsPage": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "QRCodeDetails",
                    "viewName": "QRCodeDetails",
                    "viewLevel": 2
                }
            }
        }
    }
}